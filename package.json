{
  "name": "@waky/root",
  "root": true,
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx run-many --target=serve --parallel --maxParallel=1000 --all",
    "start:one": "nx serve",
    "build": "nx run-many --target=build --all",
    "build:one": "nx build",
    "build:nocache": "yarn build --skip-nx-cache",
    "test": "nx run-many --target=test --parallel --maxParallel=1000 --all",
    "test:one": "nx test",
    "test:e2e": "nx run-many --target=e2e --parallel --maxParallel=1000 --all",
    "lint": "nx format:write --all && nx workspace-lint && yarn lint:check --fix",
    "lint:check": "nx run-many --target=lint --all --",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx format:write && nx affected:lint --all --fix",
    "affected:dep-graph": "nx affected:dep-graph",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "clean": "rimraf dist/ .cache/ node_modules/ yarn.lock yarn-error.log package-lock.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.6.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.3",
    "@nestjs/platform-fastify": "7.2.0",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.4",
    "@nestjsx/crud": "^4.6.2",
    "@webundsoehne/nestjs-util": "^1.0.0",
    "apollo-server-fastify": "^2.17.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^7.1.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "fastify-swagger": "^2.6.0",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "keypair": "^1.0.1",
    "nest-schedule": "^0.6.4",
    "passport": "^0.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.2.7",
    "@cenk1cenk2/eslint-config": "*",
    "@nestjs/testing": "^7.2.0",
    "@nrwl/cli": "^10.4.0",
    "@nrwl/eslint-plugin-nx": "^10.4.1",
    "@nrwl/jest": "^10.4.4",
    "@nrwl/tao": "^10.4.0",
    "@nrwl/workspace": "^10.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "commitizen": "^4.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.1.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.0",
    "ts-node-dev": "^1.0.0",
    "tsc-watch": "^4.2.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.5"
  }
}
